PREFIX interop: <http://www.w3.org/ns/solid/interop#>
PREFIX acl: <http://www.w3.org/ns/auth/acl#>
PREFIX solidtrees: <https://shapetrees.example/solid/>
PREFIX acmeproject: <https://acmeproject.example/>
PREFIX acme: <https://acme.example/>
PREFIX pm-shapetrees: <https://vocab.example/pm/>

acmeproject:\#id
  a interop:Application ;
  interop:applicationName "Acme Project" ;
  interop:applicationDescription
    "Manage projects with ease" ;
  interop:applicationAuthor acme:id\#org ;
  interop:applicationAuthorName "Example Organization" ;
  interop:applicationThumbnail acme:id\#org ;
  interop:receivesAccessReceipt interop:ReceiptInRegistration ;
  interop:hasAccessNeedGroup <#need-group-pm> .

<#need-group-pm>
  a interop:AccessNeedGroup ;
  interop:accessNecessity interop:accessRequired ;
  interop:accessScenario interop:PersonalAccess ;
  interop:authenticatesAs interop:Pilot ;
  interop:hasAccessDecoratorIndex pm:index ;
  interop:hasAccessNeed <#need-project> .

<#need-project>
  a interop:AccessNeed ;
  interop:inAccessNeedGroup <#need-group-pm> ;
  interop:registeredShapeTree pm:ProjectTree ;
  interop:hasAccessNeedDecorator pm:index\#projectDecorator ;
  interop:accessNecessity interop:accessRequired ;
  interop:accessMode acl:read, acl:write, acl:Control ;
  interop:hasReferencedAccessNeed
      <#need-issue>, <#need-task>, <#need-release>, <#need-milestone> .

<#need-issue>
  a interop:ReferencedAccessNeed ;
  interop:hasAccessNeed <#need-project> ;
  interop:hasAccessNeedDecorator pm:index\#issueDecorator ;
  interop:registeredShapeTree pm:IssueTree ;
  interop:accessNecessity interop:accessRequired ;
  interop:accessMode acl:read, acl:write, acl:Control .

<#need-task>
  a interop:ReferencedAccessNeed ;
  interop:hasAccessNeed <#need-project> ;
  interop:hasAccessNeedDecorator pm:index\#taskDecorator ;
  interop:registeredShapeTree pm:TaskTree ;
  interop:accessNecessity interop:accessRequired ;
  interop:accessMode acl:read, acl:write, acl:Control .

<#need-release>
  a interop:ReferencedAccessNeed ;
  interop:hasAccessNeed <#need-project> ;
  interop:hasAccessNeedDecorator pm:index\#releaseDecorator ;
  interop:registeredShapeTree pm:ReleaseTree ;
  interop:accessNecessity interop:accessRequired ;
  interop:accessMode acl:read, acl:write, acl:Control .

<#need-milestone>
  a interop:ReferencedAccessNeed ;
  interop:hasAccessNeed <#need-project> ;
  interop:hasAccessNeedDecorator pm:index\#milestoneDecorator ;
  interop:registeredShapeTree pm:MilestoneTree ;
  interop:accessNecessity interop:accessRequired ;
  interop:accessMode acl:read, acl:write, acl:Control .
