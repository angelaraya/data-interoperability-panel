PREFIX interop: <http://www.w3.org/ns/solid/interop#>
PREFIX alice: <https://alice.example/>
PREFIX alice-auth: <https://auth.alice.example/>
PREFIX alice-inbox: <https://alice.example/inbox/>
PREFIX authz-agent: <https://trusted.example/>

alice:\#id
  a interop:Agent ;
  ######## Registry Sets ########
  interop:hasApplicationRegistrySet alice:application-registry-set ;
  interop:hasDataRegistrySet alice:data-registry-set ;
  interop:hasAccessGrantRegistrySet alice:grant-registry-set ;
  interop:hasAccessReceiptRegistrySet alice:receipt-registry-set ;
  interop:hasRemoteDataRegistrySet alice:remote-registry-set ;
  ######## Authorization Agent ########
  interop:hasAuthorizationAgent authz-agent: ;
  ######## Inboxes ########
  interop:hasInbox alice-inbox:general ;
  interop:hasAccessInbox alice-inbox:access ;
  ######## Options ########
  interop:receivesAccessReceipt interop:ReceiptInMessage .
