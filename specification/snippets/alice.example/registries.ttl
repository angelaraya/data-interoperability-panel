PREFIX interop: <http://www.w3.org/ns/solid/interop#>
PREFIX alice: <https://alice.example/>
PREFIX alice-work: <https://work.alice.example/>
PREFIX alice-personal: <https://personal.alice.example/>

alice:registries
  a interop:RegistrySet;
  interop:hasApplicationRegistry alice:applications\/ ;
  interop:hasAccessGrantRegistry alice:grants\/ ;
  interop:hasAccessReceiptRegistry alice:receipts\/ ;
  interop:hasRemoteDataRegistry alice:remote\/ ;
  interop:hasDataRegistry
    alice-work:data\/ ,
    alice-personal:data\/ .
